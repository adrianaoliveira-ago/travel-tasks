{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended"
  ],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "jest"
  ],
  "rules": {},
  "ignorePatterns": [
    "**/*.svg",
    "**/*.css",
    "**/*.png"
  ],
  "settings": {
    "react": {
      "version": "detect"
    },
    "jest": {
      "version": "detect"
    }
  }
}
